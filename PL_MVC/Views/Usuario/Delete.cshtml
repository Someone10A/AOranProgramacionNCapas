@model ML.Usuario
<div class="alert alert-danger" role="alert">
    <div class="modal-body">
        <p>@ViewBag.Mensaje</p>
    </div>
    <div class="modal-footer">
        @Html.ActionLink("Eliminar", "Delete", "Usuario", new { @class = "btn btn-danger" })
        @Html.ActionLink("Cancelar", "GetAll", "Usuario", new { @class = "btn btn-success" })
    </div>
</div>
